{
  "extends": "next/core-web-vitals",
  "rules": {
	"indent": [
	  "error",
	  "tab"
	],
	"linebreak-style": [
	  "error",
	  "unix"
	],
	"quotes": [
	  "error",
	  "single"
	],
	"semi": [
	  "error",
	  "always"
	],
	"comma-dangle": [
	  "error",
	  "never"
	],
	"no-multi-spaces": [
	  "error"
	],
	"no-unused-vars": [
	  "error",
	  {
		"argsIgnorePattern": "^_"
	  }
	]
  }
}
